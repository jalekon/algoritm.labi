#include <iostream>
using namespace std;

int main()
{
	float a, b, c, max, min;
	cout << "vvedite zna4enie" << endl;
	cin >> a >> b >> c;
	max = a;
	min = a;
	if (b > max)
		max = b;
	if (c > max)
		max = c;
	cout << "max=" << max << endl;
	if (b < min)
		min = b;
	if (c < min)
		min = c;
	cout << "min=" << min << endl;
}
