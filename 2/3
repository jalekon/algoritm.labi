#include <iostream>
using namespace std;

int main()
{
	int n, a;
	cout << "vvedite zna4enie <=100" << endl;
	cin >> n;
	if ((11 <= n) & (n < 15))
		cout << n << " let";
	else
	{
		a = n % 10;
		if (a == 1)
			cout << n << " god";
		else
			if ((1 < a) & (a < 5))
				cout << n << " goda";
			else
				cout << n << " let";
	}
}
